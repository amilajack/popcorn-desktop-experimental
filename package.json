    "css-modules-require-hook": "^4.0.6",
    "devtron": "^1.4.0",
    "dotenv": "^4.0.0",
    "download": "^6.2.2",
    "electron": "1.6.10",
    "electron-builder": "^18.6.2",
    "electron-debug": "^1.1.0",
    "electron-devtools-installer": "^2.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-formatter-pretty": "^1.1.0",
